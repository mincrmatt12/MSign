	url = https://github.com/espressif/ESP8266_RTOS_SDK
	ignore = dirty
[submodule "vendor/FreeRTOS"]
	path = vendor/FreeRTOS
	url = https://github.com/FreeRTOS/FreeRTOS-Kernel
